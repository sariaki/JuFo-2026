      \strng{namehash}{78cf76bd8eb3eb10be64f596ecf740c7}
      \strng{fullhash}{a84fee1457964b99ff618db2bfaf8ca2}
      \strng{bibnamehash}{78cf76bd8eb3eb10be64f596ecf740c7}
      \strng{authorbibnamehash}{78cf76bd8eb3eb10be64f596ecf740c7}
